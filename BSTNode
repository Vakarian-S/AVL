#include "BST.h"
#include "BSTNode.h"

BSTNode::BSTNode() {
}

BSTNode::BSTNode(int data) {
    this->data = data;
    this->leftChild = nullptr;
    this->rightChild = nullptr;
    this->bf = 0;
}

BSTNode::BSTNode(const BSTNode& orig) {
}

BSTNode::~BSTNode() {
}

